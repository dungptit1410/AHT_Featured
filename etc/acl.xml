<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Catalog::catalog" title="Catalog" translate="title" sortOrder="30">
                    <resource id="Magento_Catalog::catalog_inventory" title="Inventory" translate="title" sortOrder="10">
                        <resource id="AHT_Featured::featured" title="Featured Product" translate="title" sortOrder="30">
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                    <resource id="Magento_Config::config">
                        <resource id="AHT_Featured::config_featured" title="Product Featured Section" />
                    </resource>
                    </resource>
			    </resource>	
            </resource>
        </resources>
    </acl>
</config>
